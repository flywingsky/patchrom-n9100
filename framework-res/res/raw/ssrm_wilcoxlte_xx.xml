<LevelsModel>

    <steps>
        <step id="0" temp="430"/>
        <step id="1" temp="450"/>
        <step id="2" temp="460"/>
        <step id="3" temp="470"/>
        <step id="4" temp="480"/>
        <step id="5" temp="500"/>
        <step id="6" temp="520"/>
        <step id="7" temp="530"/>
        <step id="8" temp="540"/>
        <step id="9" temp="550"/>
        <step id="10" temp="570"/>
        <step id="11" temp="580"/>
        <step id="12" temp="610"/>


    </steps>

    <writers>
        <writer name="SIOPLevel"/>
        <writer name="CPUFreqMax"/>
        <writer name="GPUFreqMax"/>
        <writer name="Flash"/>
        <writer name="Recording"/>
        <writer name="LCDBrightness"/>
        <writer name="Charging"/>
        <writer name="ACL"/>
    </writers>

    <conditions>
        <condition name="HeavyUser"/>
        <condition name="GHeavyUserUp"/>
        <condition name="GHeavyUserDown"/>
        <condition name="BrowserMode"/>
        <condition name="ScreenOff"/>
        <condition name="AppStart"/>
    </conditions>

    <levels>
        <!-- The default layer is always at the bottom of the stack. -->
        <!-- 'condition' tag must not be used. -->
        <level default="true">
            <battery>
                <state step="0">
                    <settings>
                        <setting writer="SIOPLevel"    value="0"/>
                        <setting writer="CPUFreqMax"    value="-1"/>
                        <setting writer="GPUFreqMax"    value="-1"/>
                        <setting writer="LCDBrightness" value="-1"/>
                        <setting writer="Charging"      value="100"/>
                        <setting writer="Flash"         value="1"/>
                        <setting writer="Recording"     value="1"/>
                        <setting writer="ACL"           value="0"/>
                    </settings>
                </state>
                <state step="5">
                    <settings>
                        <setting writer="SIOPLevel"    value="1"/>
                        <setting writer="CPUFreqMax"    value="1400000"/>
                        <setting writer="GPUFreqMax"    value="-1"/>
                        <setting writer="LCDBrightness" value="180"/>
                        <setting writer="Charging"      value="100"/>
                        <setting writer="Flash"         value="1"/>
                        <setting writer="Recording"     value="1"/>
                        <setting writer="ACL"           value="0"/>
                    </settings>
                </state>
                <state step="6">
                    <settings>
                        <setting writer="SIOPLevel"    value="2"/>
                        <setting writer="CPUFreqMax"    value="1200000"/>
                        <setting writer="GPUFreqMax"    value="-1"/>
                        <setting writer="LCDBrightness" value="160"/>
                        <setting writer="Charging"      value="50"/>
                        <setting writer="Flash"         value="1"/>
                        <setting writer="Recording"     value="1"/>
                        <setting writer="ACL"           value="0"/>
                    </settings>
                </state>
                <state step="8">
                    <settings>
                        <setting writer="SIOPLevel"    value="3"/>
                        <setting writer="CPUFreqMax"    value="1000000"/>
                        <setting writer="GPUFreqMax"    value="-1"/>
                        <setting writer="LCDBrightness" value="140"/>
                        <setting writer="Charging"      value="30"/>
                        <setting writer="Flash"         value="0"/>
                        <setting writer="Recording"     value="1"/>
                        <setting writer="ACL"           value="0"/>
                    </settings>
                </state>
                <state step="11">
                    <settings>
                        <setting writer="SIOPLevel"    value="4"/>
                        <setting writer="CPUFreqMax"    value="800000"/>
                        <setting writer="GPUFreqMax"    value="-1"/>
                        <setting writer="LCDBrightness" value="140"/>
                        <setting writer="Charging"      value="30"/>
                        <setting writer="Flash"         value="0"/>
                        <setting writer="Recording"     value="1"/>
                        <setting writer="ACL"           value="0"/>
                    </settings>
                </state>
                <state step="12">
                    <settings>
                        <setting writer="SIOPLevel"    value="5"/>
                        <setting writer="CPUFreqMax"    value="800000"/>
                        <setting writer="GPUFreqMax"    value="-1"/>
                        <setting writer="LCDBrightness" value="140"/>
                        <setting writer="Charging"      value="0"/>
                        <setting writer="Flash"         value="0"/>
                        <setting writer="Recording"     value="1"/>
                        <setting writer="ACL"           value="0"/>
                    </settings>
                </state>
            </battery>
        </level>

       <level>
            <condition name="HeavyUser"/>
            <battery>
                <state step="0">
                    <settings>
                    <setting writer="CPUFreqMax"    value="-1"/>
                        <setting writer="GPUFreqMax"    value="-1"/>
                        <setting writer="LCDBrightness" value="-1"/>
                        <setting writer="Charging"      value="100"/>
                        <setting writer="Flash"         value="1"/>
                        <setting writer="Recording"     value="1"/>
                        <setting writer="ACL"           value="0"/>
                    </settings>
                </state>
                <state step="1">
                    <settings>
                       <setting writer="CPUFreqMax"    value="-1"/>
                        <setting writer="GPUFreqMax"    value="-1"/>
                        <setting writer="LCDBrightness" value="180"/>
                        <setting writer="Charging"      value="50"/>
                        <setting writer="Flash"         value="1"/>
                        <setting writer="Recording"     value="1"/>
                        <setting writer="ACL"           value="0"/>
                    </settings>
                </state>
                <state step="2">
                    <settings>
                        <setting writer="CPUFreqMax"    value="-1"/>
                        <setting writer="GPUFreqMax"    value="-1"/>
                        <setting writer="LCDBrightness" value="165"/>
                        <setting writer="Charging"      value="30"/>
                        <setting writer="Flash"         value="1"/>
                        <setting writer="Recording"     value="1"/>
                        <setting writer="ACL"           value="0"/>
                    </settings>
                </state>
                <state step="3">
                    <settings>
                        <setting writer="CPUFreqMax"    value="-1"/>
                        <setting writer="GPUFreqMax"    value="-1"/>
                        <setting writer="LCDBrightness" value="150"/>
                        <setting writer="Charging"      value="0"/>
                        <setting writer="Flash"         value="1"/>
                        <setting writer="Recording"     value="1"/>
                        <setting writer="ACL"           value="0"/>
                    </settings>
                </state>
                <state step="6">
                    <settings>
                        <setting writer="CPUFreqMax"    value="1200000"/>
                        <setting writer="GPUFreqMax"    value="-1"/>
                        <setting writer="LCDBrightness" value="120"/>
                        <setting writer="Charging"      value="0"/>
                        <setting writer="Flash"         value="1"/>
                        <setting writer="Recording"     value="1"/>
                        <setting writer="ACL"           value="0"/>
                    </settings>
                </state>
                <state step="7">
                    <settings>
                        <setting writer="CPUFreqMax"    value="1000000"/>
                        <setting writer="GPUFreqMax"    value="-1"/>
                        <setting writer="LCDBrightness" value="120"/>
                        <setting writer="Charging"      value="0"/>
                        <setting writer="Flash"         value="0"/>
                        <setting writer="Recording"     value="1"/>
                        <setting writer="ACL"           value="1"/>
                    </settings>
                </state>
                <state step="9">
                    <settings>
                        <setting writer="CPUFreqMax"    value="800000"/>
                        <setting writer="GPUFreqMax"    value="320000000"/>
                        <setting writer="LCDBrightness" value="120"/>
                        <setting writer="Charging"      value="30"/>
                        <setting writer="Flash"         value="0"/>
                        <setting writer="Recording"     value="1"/>
                        <setting writer="ACL"           value="1"/>
                    </settings>
                </state>
                <state step="10">
                    <settings>
                        <setting writer="CPUFreqMax"    value="800000"/>
                        <setting writer="GPUFreqMax"    value="192000000"/>
                        <setting writer="LCDBrightness" value="120"/>
                        <setting writer="Charging"      value="30"/>
                        <setting writer="Flash"         value="0"/>
                        <setting writer="Recording"     value="1"/>
                        <setting writer="ACL"           value="1"/>
                    </settings>
                </state>
            </battery>
        </level>

        <level>
            <condition name="GHeavyUserUp"/>
            <battery>
                <state step="3">
                    <settings>
                        <setting writer="GPUFreqMax"    value="-1" option="released"/>
                    </settings>
                </state>
                <state step="6">
                    <settings>
                        <setting writer="GPUFreqMax"    value="-1" option="unchanged"/>
                    </settings>
                </state>
                <state step="7">
                    <settings>
                        <setting writer="GPUFreqMax"    value="320000000" option="triggered"/>
                    </settings>
                </state>

            </battery>
        </level>

        <level>
            <condition name="GHeavyUserDown"/>
            <battery>
                <state step="0">
                    <settings>
                        <setting writer="GPUFreqMax"    value="320000000"/>
                    </settings>
                </state>
            </battery>
        </level>

        <level>
            <condition name="BrowserMode"/>
            <battery>
                <state step="0">
                    <settings>
                        <setting writer="LCDBrightness" value="249"/>
                    </settings>
                </state>
            </battery>
        </level>

        <level>
            <condition name="ScreenOff"/>
            <battery>
                <state step="0">
                    <settings>
                        <setting writer="Charging" value="100" option="forced"/>
                    </settings>
                </state>
            </battery>
        </level>

        <level>
            <condition name="AppStart"/>
            <battery>
                <state step="0">
                    <settings>
                        <setting writer="CPUFreqMax"    value="-1"/>
                        <setting writer="GPUFreqMax"    value="-1"/>
                    </settings>
                </state>
                <state step="10">
                    <settings>
                    </settings>
                </state>
            </battery>
        </level>

    </levels>

</LevelsModel>